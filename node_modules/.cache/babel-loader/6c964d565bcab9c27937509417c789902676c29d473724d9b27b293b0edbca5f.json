    // \t\tfunction () {\n        // \t\t\tdone(); // Accept the file\n        // \t\t},\n        // \t\tfunction () {\n        // \t\t\tdone(\n        // \t\t\t\t\"Invalid image dimensions or aspect ratio. Please upload a 1.91:1 aspect ratio image with maximum dimensions of 1200px x 800px.\"\n        // \t\t\t);\n        // \t\t}\n        // \t);\n        // },\n      }\n    };\n  },\n\n  validations: {\n    vehiclePost: {\n      colorEx: {\n        required\n      },\n      colorIn: {\n        required\n      },\n      drivetrain: {\n        required\n      },\n      accesories: {\n        required\n      }\n    },\n    vehiclePostImages: {\n      required,\n      maxLength: value => {\n        console.log(value.length > 4);\n        return !value.length > 4;\n      }\n    }\n  },\n  // mounted() {\n  // \t// this.uploadImgManually()\n\n  // creating user refresca\n  // },\n\n  methods: {\n    ...mapMutations(\"auth\", [\"SET_ALERT_MSG\", \"TOGGLE_IS_LOADING\"]),\n    ...mapActions(\"adsCrud\", [\"createAd\", \"updateAd\", \"updateVehiclePost\", \"updateVehiclePostImages\", \"clearVehiclePost\", \"fetchImageUrlListById\"]),\n    afterComplete(file) {\n      this.vehiclePostImages.push(file);\n    },\n    removeImg(file) {\n      this.vehiclePostImages = this.vehiclePostImages.filter(img => img.dataURL != file.dataURL);\n    },\n    async uploadImgManually() {\n      if (this.vehicleImages.length > 0) {\n        //add images to vuedropzone\n        this.vehicleImages.forEach(detail => {\n          this.$refs.myVueDropzone.manuallyAddFile(detail, detail.dataURL);\n        });\n      }\n    },\n    async tryCreatePost() {\n      this.submitted = true;\n      if (this.$v.$invalid) {\n        return;\n      } else {\n        try {\n          this.TOGGLE_IS_LOADING();\n          if (this.vehiceId) {\n            console.log(\"updatinging\", this.vehiclePost);\n            delete this.vehiclePost.carPicsUrls;\n            await this.updateAd({\n              vehicleData: this.vehiclePost,\n              vehicleImages: this.vehiclePostImages,\n              vehicleId: this.vehiceId\n            });\n            localStorage.removeItem(\"vehicle_id\");\n            localStorage.removeItem(\"vehicle_images\");\n          } else {\n            await this.createAd({\n              vehicleData: this.vehiclePost,\n              vehicleImages: this.vehiclePostImages\n            });\n          }\n          localStorage.removeItem(\"vehicle_images\");\n          this.clearVehiclePost({});\n          this.$router.push({\n            name: \"dashboard\"\n          });\n        } catch (error) {\n          // throw error;\n          this.SET_ALERT_MSG({\n            title: \"ERROR\",\n            type: \"error\",\n            msg: error\n          });\n        } finally {\n          this.TOGGLE_IS_LOADING();\n        }\n      }\n    }\n  },\n  computed: {\n    vehiclePost: {\n      get() {\n        return this.$store.state.adsCrud.vehiclePost;\n      },\n      set(newValue) {\n        this.updateVehiclePost(newValue);\n      }\n    },\n    vehiceId() {\n      const id = JSON.parse(localStorage.getItem(\"vehicle_id\"));\n      return id ? id : null;\n    },\n    vehicleImages() {\n      const images = JSON.parse(localStorage.getItem(\"vehicle_images\"));\n      return images ? images : [];\n    }\n  }\n};","map":{"version":3,"names":["required","mapActions","mapGetters","mapMutations","mapState","vue2Dropzone","components","props","data","submitted","vehiclePostImages","dropzoneOptions","url","thumbnailWidth","thumbnailHeight","maxFiles","acceptedFiles","headers","dictDefaultMessage","addRemoveLinks","validations","vehiclePost","colorEx","colorIn","drivetrain","accesories","maxLength","value","console","log","length","methods","afterComplete","file","push","removeImg","filter","img","dataURL","uploadImgManually","vehicleImages","forEach","detail","$refs","myVueDropzone","manuallyAddFile","tryCreatePost","$v","$invalid","TOGGLE_IS_LOADING","vehi