bars {\r\n\tcursor: pointer;\r\n\tflex: 1;\r\n}\r\n.searchIcon {\r\n\tflex: 1;\r\n\tdisplay: flex;\r\n\tjustify-content: flex-end;\r\n\tfont-size: 1.5em;\r\n}\r\n\r\n.sMedia {\r\n\tdisplay: none;\r\n\tflex: 1;\r\n\theight: 100%;\r\n\tgap: 0.5em;\r\n\tflex-direction: column;\r\n\talign-items: flex-end;\r\n\tjustify-content: center;\r\n\tmargin-right: 1em;\r\n\r\n\ti {\r\n\t\tborder: 1px solid $light;\r\n\t\tfont-size: 1em;\r\n\t\twidth: 1.5em;\r\n\t\theight: 1.5em;\r\n\t\tborder-radius: 100%;\r\n\t\ttext-align: center;\r\n\t\tpadding-top: 0.2em;\r\n\t\ttransition: all 0.3s ease-in-out;\r\n\t\tcursor: pointer;\r\n\r\n\t\t&:hover {\r\n\t\t\tcolor: $primary;\r\n\t\t}\r\n\t}\r\n}\r\n.user-logo{\r\n\timg{\r\n\t\tmax-width: 100%;\r\n\t\tobject-fit: ;\r\n\t}\r\n}\r\n.header-container {\r\n\t@include breakpoint(tablet) {\r\n\t}\r\n\t@include breakpoint(desktop) {\r\n\t\t.sMedia {\r\n\t\t\tdisplay: flex;\r\n\t\t}\r\n\t\t.header-wrapper {\r\n\t\t\tpadding: 0 15em;\r\n\t\t\tgap: 1em;\r\n\t\t}\r\n\t\t.navItems-container {\r\n\t\t\tdisplay: flex;\r\n\t\t\tflex: 3;\r\n\t\t}\r\n\r\n\t\t.fa-bars {\r\n\t\t\tdisplay: none;\r\n\t\t}\r\n\t\t.fa-search {\r\n\t\t\tdisplay: none;\r\n\t\t}\r\n\t}\r\n\t.header-wrapper {\r\n\t\t@include breakpoint(lg-device) {\r\n\t\t\tmax-width: 1600px;\r\n\t\t}\r\n\t}\r\n}\r\n</style>\r\n"],"mappings":"AACA,SAAAA,UAAA,EAAAC,UAAA,EAAAC,YAAA,EAAAC,QAAA;AACA;EACAC,KAAA;EACAC,OAAA;IACA,GAAAH,YAAA,cACA,uBACA,kBACA,kBACA;IACA,GAAAF,UAAA;IACAM,eAAAC,IAAA;MACA,IAAAC,OAAA;MACA,IACAD,IAAA,yBAAAE,UAAA,IACAF,IAAA,sBAAAE,UAAA,IACAF,IAAA,uBAAAE,UAAA,EACA;QACAD,OAAA;MACA;MACA,OAAAA,OAAA;IACA;IACAE,QAAAH,IAAA;MACA,IAAAA,IAAA;QACA,KAAAI,cAAA;QACA,KAAAC,eAAA;MACA,WAAAL,IAAA;QACA;QACA,KAAAI,cAAA;QACA,KAAAA,cAAA;UAAAE,IAAA;QAAA;QACA,KAAAD,eAAA;MACA;IACA;IACA,MAAAE,QAAAP,IAAA;MACA,IAAAA,IAAA,sBAAAQ,MAAA,CAAAC,IAAA;QACA;UACA,WAAAC,WAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;YAAAH,IAAA;UAAA;QACA,SAAAI,KAAA;UACA,MAAAA,KAAA;QACA;MACA;IACA;EACA;EACAC,QAAA;IACA,GAAAlB,QAAA;IACA,GAAAA,QAAA;IACA,G